{
  "manifest_version": 2,

  "name": "Caleb's Miva Tweaks",
  "description": "Make the Miva admin a little friendlier",
  "version": "1.0",
  "permissions": [
   "activeTab"
   ],
  "content_scripts": [
    {
      "matches": ["http://*/*admin.mvc*"],
      "js": ["build.js"],
      "run_at": "document_end"
    }
  ]
}